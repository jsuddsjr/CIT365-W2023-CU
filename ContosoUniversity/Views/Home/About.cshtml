@model IEnumerable<ContosoUniversity.Models.SchoolViewModels.EnrollmentDateGroup>

@{
    ViewData["Title"] = "About Us";
}

<h2>@ViewData["Title"]</h2>

<p>
    We understand that online learning can be challenging, which is why we offer a range of support services to help our students succeed. Our online tutoring and academic advising services provide personalized support and guidance, while our career counseling services help our students prepare for their future careers.
</p>

